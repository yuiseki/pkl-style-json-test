id = "landuse-industrial"
type = "fill"
source = "openmaptiles"
`source-layer` = "landuse"
filter {
  "all"
  new {
    "=="
    "$type"
    "Polygon"
  }
  new {
    "in"
    "class"
    "industrial"
    "garages"
    "dam"
  }
}
layout {
  visibility = "visible"
}
paint {
  `fill-color` = "hsla(49, 100%, 88%, 0.34)"
}
