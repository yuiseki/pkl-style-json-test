id = "railway_dashline"
type = "line"
metadata {
  `mapbox:group` = "b6371a3f2f5a9932464fa3867530a2e5"
}
source = "openmaptiles"
`source-layer` = "transportation"
minzoom = 13
filter {
  "all"
  new {
    "=="
    "$type"
    "LineString"
  }
  new {
    "all"
    new {
      "!has"
      "service"
    }
    new {
      "=="
      "class"
      "rail"
    }
  }
}
layout {
  `line-join` = "round"
  visibility = "visible"
}
paint {
  `line-color` = "hsl(224, 20%, 41%)"
  `line-dasharray` {
    3
    3
  }
  `line-width` {
    base = 1.3
    stops {
      new {
        16
        1.5
      }
      new {
        20
        6
      }
    }
  }
}
